    "def solution(n):\n",
    "    answer = 0\n",
    "\n",
    "    list = [0, 1]\n",
    "    for i in range(2, n+1):\n",
    "        answer = (list[i-1] + list[i-2]) % 1234567\n",
    "        list.append(answer)\n",
    "\n",
    "    answer = list[-1]\n",
    "\n",
    "    return answer"
