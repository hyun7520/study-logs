   "source": [
    "def solution(targets):\n",
    "    answer = 0\n",
    "    e = 0\n",
    "    targets.sort(key = lambda x: (x[1]))\n",
    "\n",
    "    for target in targets:\n",
    "        if target[0] >= e:\n",
    "            answer += 1\n",
    "            e = target[1]\n",
    "    \n",
    "    return answer"
   ]
