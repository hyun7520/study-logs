    "def solution(storey):\n",
    "    answer = 0\n",
    "\n",
    "    while storey:\n",
    "        div = storey%10\n",
    "        if div == 5 and storey//10%10 >= 5 or div > 5:\n",
    "            answer += 10-div\n",
    "            storey += 10-div\n",
    "        else:\n",
    "            answer += 10-div\n",
    "        storey = storey//10\n",
    "\n",
    "    return answer"
   ]
  }
